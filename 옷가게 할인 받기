import Foundation

func solution(_ price:Int) -> Int {
    if price >= 500000 {
        return Int(Double(price) - Double(price / 5))
    }
    if price >= 300000 {
        return Int(Double(price) - Double(price / 10))
    }
    if price >= 100000 {
        return Int(Double(price) - Double(price / 20))
    }
    return price
}
